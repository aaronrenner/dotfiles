#!/usr/bin/env zsh

export PATH="$HOME/.bin:$PATH"

#Tmuxinator auto completion
test -s "$HOME/.bin/tmuxinator.zsh" && source ~/.bin/tmuxinator.zsh

# Asdf version manager
test -s "$HOME/.asdf/asdf.sh" && source ~/.asdf/asdf.sh
